{
  "name": "kain/hyperf-extra",
  "description": "辅助 Hyperf 常用扩展的工具集",
  "require": {
    "php": ">=7.4",
    "hyperf/framework": "~2.0.0",
    "ramsey/uuid": "^3.8",
    "lcobucci/jwt": "^3.3",
    "phpseclib/phpseclib": "^2.0",
    "danielstjules/stringy": "^3.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "hyperf/testing": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Hyperf\\Extra\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "HyperfExtraTest\\": "tests/"
    }
  },
  "authors": [
